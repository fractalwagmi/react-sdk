{
  "name": "@fractalwagmi/fractal-sdk",
  "version": "0.2.6",
  "description": "Fractal Client SDK to install Fractal Wallet and access Fractal SDKs/APIs.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf dist && rm -rf *.tsbuildinfo",
    "prepare": "husky install && ts-patch install",
    "build": "npm run build:esm && npm run build:cjs && npm run build:styles",
    "build:esm": "tsc-esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs --target es6",
    "build:styles": "cp src/styles.css dist/styles.css",
    "postbuild": "echo '{\"type\":\"commonjs\"}' | npx json > dist/cjs/package.json && echo '{\"type\":\"module\"} ' | npx json > dist/esm/package.json",
    "prebuild:local": "[ ! -d node_modules/react.disabled ] || mv node_modules/react.disabled node_modules/react;",
    "build:local": "npm run build",
    "postbuild:local": "[ ! -d node_modules/react ] || mv node_modules/react node_modules/react.disabled",
    "lint": "eslint --fix",
    "pretest": "npm run prebuild:local",
    "posttest": "npm run postbuild:local"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fractalwagmi/fractal-sdk.git"
  },
  "keywords": [
    "fractal",
    "sdk",
    "api",
    "wallet",
    "signin",
    "login",
    "gaming"
  ],
  "bugs": {
    "url": "https://github.com/fractalwagmi/fractal-sdk/issues"
  },
  "homepage": "https://github.com/fractalwagmi/fractal-sdk#readme",
  "peerDependencies": {
    "react": ">=17.0.3 || ^18",
    "react-dom": ">=17.0.3 || ^18"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@fontsource/quattrocento-sans": "^4.5.9",
    "@fractalwagmi/fractal-auth-api": "^0.0.22",
    "@fractalwagmi/fractal-sdk-api": "^0.0.5",
    "@fractalwagmi/fractal-auth-private-web-sdk-api": "^0.0.1",
    "jest": "^28.1.3",
    "jwt-decode": "^3.1.2",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@digitak/tsc-esm": "^3.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.23",
    "@types/react": "17.0.39",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "husky": "^7.0.4",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^12.3.4",
    "live-server": "^1.2.2",
    "prettier": "^2.5.1",
    "prettier-plugin-jsdoc": "^0.3.30",
    "ts-jest": "^28.0.7",
    "ts-patch": "^2.0.1",
    "typescript": "^4.6.3",
    "typescript-transform-paths": "^3.3.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint --fix"
  }
}